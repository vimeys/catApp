.vip{
    width: 670rpx;
    height: 400rpx;
    margin: 50rpx auto 0;
    position: relative;
}
.vip>image{
    /*position: relative;*/
    /*z-index: -1;*/
    width: 100%;
    height: 100%;
}
.vip>.main{
    position: absolute;
    z-index: 10;
    top:0;
    left: 0;
    display: flex;
    flex-direction: column;
    /*justify-content: center;*/
    align-items: center;
    color:#fff;
    width: 670rpx;
    height: 400rpx;
}
.main1{
    font-size: 30rpx;
    margin-top: 30rpx;
    margin-left: 24rpx;
    align-self: flex-start;
}
.main2{
    font-size: 24prx;
    margin-left: 24rpx;
    margin-right: 24rpx;
    margin-top: 20rpx;
}
.main3{
    display: flex;
    flex-direction: row;
    align-items: flex-end;
    margin-top: 20rpx;
    font-size: 24rpx;
}
.main view:nth-child(2){
    margin-right: 20rpx;
    margin-left: 20rpx;
}
.main view text{
    font-size: 48rpx;
}
.main3 image{
    width: 42rpx;
    height: 42rpx;
}
.main4{
    font-size: 30rpx;
    margin-top:20rpx;
    padding: 10rpx 50rpx;
    border: 1px solid #fff;
    border-radius: 32rpx;
}