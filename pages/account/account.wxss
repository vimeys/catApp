/* pages/account/account.wxss */
page{
    background-color: #eee;
    padding-top: 30rpx;
    border: 2rpx #eee solid;
}
.account{
    font-size: 36rpx;
    padding: 20rpx 0 20rpx 50rpx;
    margin-top: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
}
.account-one{
    margin-bottom: 20rpx;
    display: flex;
}
.account-two{
    font-weight: bold;
    margin-bottom: 10rpx;
    color: #0092be;
}
.account-three{
    font-weight: bold;
    margin-bottom: 10rpx;
    color: #0092be;
}
.account-four{
    margin-bottom: 10rpx;
    color: #666;
}
.account-one image{
    border-radius: 50%;
    width: 200rpx;
    height: 200rpx;
}
.account-one view{
    margin-left: 50rpx;
    margin-top: 60rpx;
    font-size: 36rpx;
    font-weight: bold;
}
.fix{
    width: 100%;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    color:#fff;
    background: #00a79e;
    font-size: 38rpx;
    position: fixed;
    left: 0;
    bottom: 0;
}
.margin100{
    height: 120rpx;
}