<!--pages/platform/platform.wxml-->
<view class='platform'>
      <view class='platform-image'>
           <image style='width:400rpx;height:400rpx;' src='{{data.qrcode}}'></image>
      </view>
      <text class='platform-number' bindtap='phone' data-phone='{{data.mobile}}'>拨打电话：{{data.mobile}}</text>
</view>
